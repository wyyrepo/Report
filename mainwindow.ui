        QPushButton {
        color: #333;
        border: 2px solid lightgray;
        border-radius: 11px;
        padding: 5px;
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                              stop: 0 rgb(160, 160, 160), stop: 0.4 rgb(150, 150, 150),
                                              stop: 0.5 rgb(140, 140, 140), stop: 1.0 rgb(130, 130, 130));;}

        QPushButton:hover {
        background: qradialgradient(cx: 0.3, cy: -0.4,
        fx: 0.3, fy: -0.4,
        radius: 1.35, stop: 0 #fff, stop: 1 #bbb);}

        QPushButton:pressed {
        background: qradialgradient(cx: 0.4, cy: -0.1,
