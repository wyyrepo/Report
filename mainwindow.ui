        QComboBox {
        border: 2px solid lightgray;
        border-radius: 8px;
        padding: 1px 18px 1px 3px;
        min-width: 6em;
        color: white;
        background-color: rgb(160, 160, 160);
        margin: 0 0 0 0;}

        QComboBox:editable {
        background: rgb(220, 220, 220);}
        QComboBox:!editable {
        background: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,
                                      stop: 0 rgb(160, 160, 160), stop: 0.4 rgb(150, 150, 150),
                                      stop: 0.5 rgb(140, 140, 140), stop: 1.0 rgb(130, 130, 130));;}

        QComboBox:on{
        padding-top: 3px;
        padding-left: 4px;
        background-color: rgb(160, 160, 160);}

        QComboBox::drop-down{
        subcontrol-origin: padding;
        subcontrol-position: center right;
        width: 50px;
        height: 50px;
        right: 4px;
        border-width: 1px;
        border-color: darkgray;
        border-style: solid;
        border-radius: 8px;}

        QComboBox::down-arrow{
        image: url(:/pictures/pictures/arrow_down.png);
        height: 70px;
        width:  70px;}

        QComboBox::down-arrow:on{
        top: 1px;
        left: 1px;}

        QCombobox QLineEdit{
        left:50%;}

        QComboBox QListView{
        border-width: 3px;
        border-color: darkgray;
        border-style: solid;
        color: white;
